{
  "name": "@stephenvector/prefab",
  "version": "0.1.2",
  "description": "A set of themeable UI components for React projects.",
  "scripts": {
    "dev": "webpack-dev-server",
    "build:lib": "tsc",
    "test": "jest",
    "cleanup": "rimraf coverage lib node_modules",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenvector/prefab.git"
  },
  "author": "Stephen Vector",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stephenvector/prefab/issues"
  },
  "homepage": "https://github.com/stephenvector/prefab#readme",
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@types/draft-js": "0.10.37",
    "@types/enzyme": "3.10.3",
    "@types/jest": "24.0.19",
    "@types/react": "16.9.7",
    "@types/react-dom": "16.9.1",
    "@types/styled-components": "4.1.19",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "css-loader": "3.2.0",
    "draft-js": "0.11.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "highlight.js": "9.15.10",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-template": "6.2.0",
    "jest": "24.9.0",
    "prism-react-renderer": "1.0.1",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "rimraf": "3.0.0",
    "slate-react": "0.22.9",
    "style-loader": "1.0.0",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.0",
    "typescript": "3.6.4",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2"
  },
  "dependencies": {
    "@stephenvector/picto": "^0.1.18",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "styled-components": "4.4.0"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
